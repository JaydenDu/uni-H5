#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules/uglify-js/bin/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules/uglify-js/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules/uglify-js/bin/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules/uglify-js/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/uglify-js@https+++registry.npmmirror.com+uglify-js+-+uglify-js-3.17.4.tgz/node_modules:/Users/oracoke/mvvm/uni-H5/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
